@article{BandyopadhyayCoyle2004,
  title    = {Minimizing communication costs in hierarchically-clustered networks of wireless sensors},
  journal  = {Computer Networks},
  volume   = {44},
  number   = {1},
  pages    = {1 - 16},
  year     = {2004},
  issn     = {1389-1286},
  doi      = {https://doi.org/10.1016/S1389-1286(03)00320-7},
  url      = {http://www.sciencedirect.com/science/article/pii/S1389128603003207},
  author   = {Seema Bandyopadhyay and Edward J. Coyle},
  keywords = {Sensor networks, Clustering methods, Voronoi tessellations},
  abstract = {A multi-hop network of wireless sensors can be used to gather spatio-temporal samples of a physical phenomenon and transmit these samples to a processing center. This paper addresses the important issue of minimizing the number of transmissions required to gather one sample from each sensor. The technique used to minimize communication costs combines analytical results from stochastic geometry with a distributed, randomized algorithm for generating clusters of sensors. The minimum communication energy achieved by this approach is significantly lower than the energy costs incurred in non-clustered networks and in clustered networks produced by such algorithms as the Max–Min d-cluster algorithm.}
}

